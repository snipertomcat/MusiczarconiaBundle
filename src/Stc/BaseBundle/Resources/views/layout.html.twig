{% extends '::base.html.twig' %}
{% block title %}Base Bundle Layout{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('bundles/stcbase/js/jquery/css/ui-lightness/jquery-ui-1.10.4.custom.css') }}" rel="stylesheet" />
    <link href="{{ asset('bundles/stcbase/js/jquery/development-bundle/themes/ui-lightness/jquery-ui.css') }}" rel="stylesheet" />
    <link href="{{ asset('bundles/stcbase/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('bundles/stcbase/css/main.css') }}" rel="stylesheet" />
    {% block javascripts %}
        <script src="{{ asset('bundles/stcbase/js/jquery/js/jquery-1.10.2.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/stcbase/js/jquery/js/jquery-ui-1.10.4.custom.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/stcbase/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
    {% endblock %}
{% endblock %}

{% block head %}

{% endblock %}

{% block content %}
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            <div class="flash-{{ type }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}

    <div>
        {% block fos_user_content %}
        {% endblock fos_user_content %}
    </div>
{% endblock %}
